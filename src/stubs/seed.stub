const TABLE_NAME = 'table_name';

/**
 * Delete existing entries and seed values for `table_name`.
 *
 * @param   {object} knex
 * @returns {Promise}
 */
export function seed(knex) {
  return knex(TABLE_NAME)
    .del()
    .then(() => {
      return knex(TABLE_NAME).insert([
        {
          colName: 'rowValue',
          colName2: 'rowValue'
        },
        {
          colName: 'rowValue',
          colName2: 'rowValue'
        }
      ]);
    });
}
